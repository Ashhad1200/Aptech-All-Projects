@model eCommerce.Models.Category

<h1>
    Edit Tags
</h1>

<form method="post" class="align-content-center">
    <div class="mb-3">
        <label for="tagName" class="form-label">Category Name Here</label>
        <input type="text" class="form-control w-25" asp-for="CategoryName" id="CategoryNameInput" aria-describedby="emailHelp">
    </div>
    <button type="submit" class="btn btn-outline-primary">Add 😊</button>
</form>
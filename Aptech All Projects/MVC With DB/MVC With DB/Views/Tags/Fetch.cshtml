@model IEnumerable<MVC_With_DB.Models.Tag>

<table class="table table-success table-striped table-hover table-bordered">
    <thead>
        <tr>
            <th>ID</th>
            <th>Tag Name</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>
                    <a class="btn btn-outline-primary" href="#">Edit</a>
                    <a class="btn btn-outline-danger" href="@Url.Action("Delete", new { id = item.Id })">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
